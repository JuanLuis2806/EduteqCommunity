<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.2" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd">
  <!-- Define Persistence Unit -->
  <persistence-unit name="my_persistence_unit" transaction-type="JTA">
    <jta-data-source>jdbc/eduteq</jta-data-source>
    <class>entities.PublicacionUsuario</class>
    <class>entities.Perfil</class>
    <class>entities.Division</class>
    <class>entities.Eventos</class>
    <class>entities.Usuario</class>
    <class>entities.Carrera</class>
    <class>entities.Estado</class>
    <class>entities.Mercado</class>
    <class>entities.Publicacion</class>
    <class>entities.TipoEvento</class>
    <class>entities.TipoUsuario</class>
    <class>entities.ComentariosMercado</class>
  </persistence-unit>
</persistence>
